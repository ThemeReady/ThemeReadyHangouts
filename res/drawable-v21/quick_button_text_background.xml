<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4.0dip" />
            <solid android:color="@color/quick_button_text_background_pressed_color" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="4.0dip" />
            <solid android:color="@color/quick_button_text_background_unpressed_color" />
        </shape>
    </item>
</selector>